{
  "name": "vue-tpl",
  "author": "毛瑞 <Maorey@Foxmail.com>",
  "version": "0.2.0",
  "private": false,
  "license": "MIT",
  "keywords": [
    "vue2",
    "vuex3",
    "vuerouter",
    "typescript3",
    "template"
  ],
  "description": "vue + vuex + vue router + TypeScript(支持 JavaScript) 模板",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Maorey/vue-tpl.git"
  },
  "bugs": {
    "url": "https://github.com/Maorey/vue-tpl/issues"
  },
  "scripts": {
    "dev": "npx --max_old_space_size=4096 vue-cli-service serve --open",
    "lint": "npx --max_old_space_size=4096 stylelint **/*.vue **/*.scss **/*.css --cache --cache-location node_modules/.cache/stylelint/ --fix && npx --max_old_space_size=4096 vue-cli-service lint --fix",
    "build": "npx --max_old_space_size=4096 vue-cli-service build --no-unsafe-inline",
    "test:e2e": "npx --max_old_space_size=4096 vue-cli-service test:e2e",
    "test:unit": "npx --max_old_space_size=4096 vue-cli-service test:unit --detectOpenHandles --coverage"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.4.5",
    "crypto-js": "^3.1.9-1",
    "d3": "^5.14.2",
    "echarts": "^4.5.0",
    "element-ui": "^2.13.0",
    "jsencrypt": "^3.0.0-rc.1",
    "luma.gl": "^7.3.2",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pixi.js": "^5.2.0",
    "pixi.js-legacy": "^5.2.0",
    "register-service-worker": "^1.6.2",
    "three": "^0.111.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-i18n": "^8.15.1",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^0.11.0",
    "zdog": "^1.1.1",
    "zrender": "^4.1.2"
  },
  "devDependencies": {
    "@types/crypto-js": "^3.1.43",
    "@types/d3": "^5.7.2",
    "@types/echarts": "^4.4.1",
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "@vue/cli-plugin-babel": "^4.1.1",
    "@vue/cli-plugin-e2e-cypress": "^4.1.1",
    "@vue/cli-plugin-eslint": "^4.1.1",
    "@vue/cli-plugin-pwa": "^4.1.1",
    "@vue/cli-plugin-router": "^4.1.1",
    "@vue/cli-plugin-typescript": "^4.1.1",
    "@vue/cli-plugin-unit-jest": "^4.1.1",
    "@vue/cli-plugin-vuex": "^4.1.1",
    "@vue/cli-service": "^4.1.1",
    "@vue/eslint-config-standard": "^5.0.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "compression-webpack-plugin": "^3.0.0",
    "eslint": "^6.7.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "fibers": "^4.0.2",
    "hard-source-webpack-plugin": "^0.13.1",
    "lint-staged": "^9.5.0",
    "regenerate": "^1.4.0",
    "regjsgen": "^0.5.1",
    "regjsparser": "^0.6.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "stylelint": "^12.0.0",
    "stylelint-config-scss-maorey": "^1.1.1",
    "stylelint-webpack-plugin": "^1.1.0",
    "typescript": "^3.7.2",
    "unicode-match-property-ecmascript": "^1.0.4",
    "unicode-match-property-value-ecmascript": "^1.1.0",
    "vue-template-compiler": "^2.6.10",
    "vue-tsx-support": "^2.3.2",
    "webpack-remove-strict-mode-plugin": "^1.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,vue}": [
      "npx --max_old_space_size=4096 vue-cli-service lint --fix",
      "git add"
    ],
    "*.{vue,scss,css}": [
      "npx --max_old_space_size=4096 stylelint **/*.vue **/*.scss **/*.css --cache --cache-location node_modules/.cache/stylelint/ --fix",
      "git add"
    ]
  }
}
